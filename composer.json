{
	"name": "p2made/yii2-p2y2-things",
	"description": "A bunch of CSS & JS resources wrapped as Yii 2 AssetBundles &/or Widgets.",
	"type": "yii2-extension",
	"keywords": [
		"yii2", "yii", "extension", "PHP", "asset", "bundle", "admin", "alerts",
		"Bootstrap Social", "calendar", "charts", "data", "dataTables", "ector", "ector graphics",
		"excanvas", "Flot", "frontend", "FullCalendar", "graphics", "Holder.js", "html5shiv",
		"menu", "metisMenu", "moment", "moment-timezone", "moment.js", "Morris", "Morris.JS",
		"placeholder", "placeholder-image", "raphael", "Respond.JS", "social", "SweetAlert",
		"tables", "theme", "time", "timezone", "ui", "user-interface", "widget"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Pedro Plowman",
			"email": "p2opensource@gmail.com",
			"homepage": "https://github.com/p2made"
		}
	],
	"support": {
		"issues": "https://github.com/p2made/yii2-p2y2-things/issues?state=open",
		"source": "https://github.com/p2made/yii2-p2y2-things"
	},

	"version": "3.0.0-dev",
	"minimum-stability": "dev",
	"prefer-stable" : true,

	"require": {
		"yiisoft/yii2": "^2.0",
		"p2made/yii2-p2y2-base": "^1.1",
		"p2made/yii2-p2y2-moment": "^2.0",
		"bower-asset/jquery": "3.3.*@stable",
		"twbs/bootstrap": "^4.1",
		"thomaspark/bootswatch": "^4.1",
		"bpampuch/pdfmake": "^0.1.24",
		"components/flag-icon-css": "^2.8",
		"dimsemenov/magnific-popup": "^1.1",
		"drmonty/animate.css": "^3.5",
		"drmonty/ekko-lightbox": "^5.0",
		"fortawesome/font-awesome": "^4.7",
		"happyworm/jPlayer": "^2.9",
		"imsky/holder": "^2.9",
		"moontoast/math": "^1.1",
		"nostalgiaz/bootstrap-switch": "^3.3",
		"onokumus/metismenu": "^2.6",
		"philippfrenzel/yii2fullcalendar": "^3.0",
		"ramsey/uuid": "^3.5",
		"rmrevin/yii2-fontawesome": "^2.17",
		"stuk/jszip": "^3.0"
	},

	"repositories": [
		{
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	],

	"autoload": {
		"psr-4": {
			"p2m\\": ""
		}
